{
  "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "outDir": "build",
      "sourceMap": true,
      "target": "es5",
      "downlevelIteration": true
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules",
  ]
}
